CREATE TABLE <%= keyspace %>.edges(
  id         bigint,
	source     bigint,
	target     bigint,
	partition  int,
	comp       map<text, frozen<map  <text, frozen <trueno_tuple>>>>,
	prop       map<text, frozen<trueno_tuple>>,
	meta       map<text, frozen<trueno_tuple>>,

  PRIMARY KEY (source, target)
);
